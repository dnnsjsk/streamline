{
  "rules": {
    "@stencil/required-jsdoc": 0,
    "@stencil/ban-prefix": 0,
    "@stencil/async-methods": 0,
    "@stencil/strict-mutable": 0,
    "react/jsx-no-bind": 0,
    "lines-between-class-members": 0,
    "no-eval": 0
  },
  "env": {
    "jest": true,
    "browser": true,
    "es2020": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": [
    "plugin:@stencil/recommended",
    "standard",
    "prettier"
  ]
}
