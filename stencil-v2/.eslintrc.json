{
  "rules": {
    "@stencil/required-jsdoc": 0,
    "@stencil/ban-prefix": 0
  },
  "env": {
    "jest": true,
    "browser": true,
    "es2020": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "extends": ["plugin:@stencil/recommended", "standard"]
}
